{
  "name": "dhawton/laravel-logicbox",
  "description": "Library that let you connect your laravel application to Logicbox's API with ease",
  "homepage": "http://www.danielhawton.com",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Hawton",
      "email": "daniel@hawton.org"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^7.2 || ^8.0",
    "vlucas/phpdotenv": "^4.0",
    "nesbot/carbon": "^2.0 || ^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0 || ^8.0 || ^9.0"
  },
  "autoload": {
    "psr-4": {
      "Dhawton\\LaravelLb\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Dhawton\\LaravelLb\\LaravelLbServiceProvider"
      ]
    }
  }
}
